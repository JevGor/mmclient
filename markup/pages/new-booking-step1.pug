                  a.booking__order-nav-items(href="new-booking-step3.html") 3
                  a.booking__order-nav-items(href="new-booking-step4.html") 4
                  a.booking__order-nav-items(href="new-booking-step5.html") 5
                  
              .booking__order-cart
                .booking__order-cart-title.el.el--p10-15
                  p.text.text--blueLight14.text--uppercase Massage
                .booking__order-cart-description
                  .booking__order-cart-description-text-wrap
                    p.text.text--white14 Lorem ipsum dolor sit amet, consectetur adipiscing elit. Vestibulum pulvinar scelerisque quam vel porttitor
                .row.el--p20-15
                  a.button.button--blue.text.text--blueLight20.text--uppercase.text--center.el.el--resizeMr3(href="explore.html") Explore
                  a.button.button--blue.text.text--blueLight20.text--uppercase.text--center(href="#") Book
                  
              .booking__order-cart.booking__order-cart--blue
                .booking__order-cart-title.el.el--p10-15
                  p.text.text--blueLight14.text--uppercase Massage
                .booking__order-cart-description.column
