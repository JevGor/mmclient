doctype html
html(class="no-js", lang="ru")
  include ../components/head/head.pug
  head
    +head(head.defaults)
  body.page
    |%=symbols=%
    
    .wrapper
      .content.content--newBooking
        .el.el--mb20
          header.page-header
            .page-header__content
              .container
                .page-header__content.page-header__content--body
                  .page-header__content-arrow
                    a.arrow(href="#")
                      != pugHelpers.Icon.call(locals, {iconName: 'arrow-icon', className: 'arrow-icon'})
                  h2.text.text--white24 Sat 15 July 2017
        main.page-content.el.el--mb50
          nav.menu.menu--hidden
            .el.el--mb35
              .menu__head
                span.menu__exit
                .menu__head-title
                  h2.text.text--white20 Indy Sports
                    span.menu__head-icon
                      != pugHelpers.Icon.call(locals, {iconName: 'menu-head-icon', className: 'menu-head-icon'})
            ul.menu__list
              li.menu__items
                a.text.text--blueLight18(href="wallet.html") Wallet
              li.menu__items
                a.text.text--blueLight18(href="referal.html") Referal
              li.menu__items
                a.text.text--blueLight18(href="corporate.html") Corporate
              li.menu__items
                a.text.text--blueLight18(href="settings.html") Settings
              li.menu__items
                a.text.text--blueLight18(href="faq.html") FAQ
              li.menu__items.menu__items--logout
                a.button.button--red.text.text--red16.text--uppercase.text--center(href="#") Log out
                
          .booking.booking--newBooking
            .container
            
              .booking__order-nav.el.el--mb30
                .row.row--jc-c
                  a.booking__order-nav-items.booking__order-nav-items--finish(href="new-booking-step1.html") 1
                  a.booking__order-nav-items.booking__order-nav-items--active(href="new-booking-step2.html") 2 Date & time
                  a.booking__order-nav-items(href="new-booking-step3.html") 3
                  a.booking__order-nav-items(href="new-booking-step4.html") 4
                  a.booking__order-nav-items(href="new-booking-step5.html") 5
                  
              .booking__order-date.el--mb20
                .booking__order-date-title.el.el--mb20
                  .row.row--jc-sa
                    p.text.text--grey24 Date
                    p.text.text--grey24 Time
                    
                .booking__order-date-description.el.el--p25-0
                  .row.row--jc-sa.row--grow
                  
                    .booking__order-date-description-block.column
                      .row.row--grow
                        .booking__order-date-description-block-text-wrap.column.column--ai-c
                          p.booking__order-date-description-text.booking__order-date-description-text--op25 Dec
                          p.booking__order-date-description-text.booking__order-date-description-text--op50 Dec
                          .booking__order-date-description-block-text-input-wrap
                            input.booking__order-date-description-block-text-input(type="text" value="Dec")
                          p.booking__order-date-description-text.booking__order-date-description-text--op50 Dec
                          p.booking__order-date-description-text.booking__order-date-description-text--op25 Dec
                        .booking__order-date-description-block-text-wrap.column.column--ai-c
                          p.booking__order-date-description-text.booking__order-date-description-text--op25 31 fr
                          p.booking__order-date-description-text.booking__order-date-description-text--op50 31 sa
                          .booking__order-date-description-block-text-input-wrap
                            input.booking__order-date-description-block-text-input(type="text" value="31 su")
                          p.booking__order-date-description-text.booking__order-date-description-text--op50 31 mo
                          p.booking__order-date-description-text.booking__order-date-description-text--op25 31 tu
                            
                    .booking__order-date-description-block.column
                      .row.row--grow  
                        .booking__order-date-description-block-text-wrap.column.column--ai-c
                          p.booking__order-date-description-text.booking__order-date-description-text--op25 6
                          p.booking__order-date-description-text.booking__order-date-description-text--op50 7
                          .booking__order-date-description-block-text-input-wrap.booking__order-date-description-block-text-input-wrap--before
                            input.booking__order-date-description-block-text-input(type="text" value="8")
                          p.booking__order-date-description-text.booking__order-date-description-text--op50 9
                          p.booking__order-date-description-text.booking__order-date-description-text--op25 10
                        .booking__order-date-description-block-text-wrap.column.column--ai-c
                          p.booking__order-date-description-text.booking__order-date-description-text--op25 57
                          p.booking__order-date-description-text.booking__order-date-description-text--op50 58
                          .booking__order-date-description-block-text-input-wrap
                            input.booking__order-date-description-block-text-input(type="text" value="59")
                          p.booking__order-date-description-text.booking__order-date-description-text--op50 00
                          p.booking__order-date-description-text.booking__order-date-description-text--op25 01
                  
              .booking__order-date-links
                .row.row--jc-sa
                  .booking__order-date-link
                    a.button.button--blue.text.text--blueLight.text--fw400.text--uppercase.text--center(href="new-booking-step1.html") Pre selected
                  .booking__order-date-link
                    a.button.button--blue.text.text--blueLight.text--fw400.text--uppercase.text--center(href="new-booking-step3.html") Next
        
      footer.page-footer
        .footer-content
          .footer-content__description
            .footer-content__description--body
              .container
                ul.nav
                  li.nav__items
                    a.nav__item.nav__item--newBooking(href="new-booking-start.html")
                      != pugHelpers.Icon.call(locals, {iconName: 'new-booking', className: 'new-booking'})
                  li.nav__items
                    a.nav__item.nav__item--bookingList(href="booking-list.html")
                      != pugHelpers.Icon.call(locals, {iconName: 'booking-list', className: 'booking-list'})
                  li.nav__items
                    a.nav__item.nav__item--message(href="messages.html")
                      != pugHelpers.Icon.call(locals, {iconName: 'message-icon', className: 'message-icon'})
                  li.nav__items
                    a.nav__item.nav__item--menu(href="#")
                      != pugHelpers.Icon.call(locals, {iconName: 'menu-icon', className: 'menu-icon'})
    
    script(src='%=static=%js/separate-js/svg4everybody.min.js')
    script svg4everybody();

    //- Main scripts. You can replace it, but I recommend you to leave it here
    script(src='%=static=%js/main%=hash=%%=min=%.js')
